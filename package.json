{
  "name": "rxjs",
  "version": "1.0.0",
  "license": "MIT",
  "author": "jjuidev <jjuidev@gmail.com>",
  "repository": "git@github.com-jjuidev:jjuidev/rxjs.git",
  "main": "./dist/main.js",
  "scripts": {
    "build": "rimraf build && tsc && tsc-alias",
    "dev": "tsnd --respawn --cls --rs -r tsconfig-paths/register ./src/main.ts",
    "format": "eslint --fix \"src/**/*.{js,ts,jsx,tsx}\" && prettier --write \"src/**/*.{js,ts,jsx,tsx}\"",
    "prepare": "husky",
    "start": "node ."
  },
  "dependencies": {
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.39.1",
    "@jjuidev/node-devtools": "^1.3.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-autofix": "^2.2.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prefer-arrow-functions": "^3.9.1",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "reflect-metadata": "^0.2.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "package.json": [
      "npx prettier-package-json --write"
    ]
  }
}
